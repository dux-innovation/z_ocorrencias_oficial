			<m:Table id="ocorrenciasTable"
        		inset="false" 
        		noDataText="Nenhuma ocorrência encontrada"
        		items="{  
        			path: '/Ocorrencias', 
        			sorter: {
        				path: 'IdOcorre'
        			}
        		}"
        		growingThreshold="3"
                growing="true"
                itemPress="onItemPress">
        		<m:headerToolbar>
        			<m:Toolbar>
        				<m:Title text="Ocorrências" level="H2"/>
        			</m:Toolbar>
        		</m:headerToolbar>
        		<m:columns>
        			<m:Column>
        				<m:Text text="Data" />
        			</m:Column>
        			<m:Column>
        				<m:Text text="Unidade" />
        			</m:Column>
        			<m:Column>
        				<m:Text text="Resumo" />
        			</m:Column>
        			<m:Column>
        				<m:Text text="Status" />
        			</m:Column>
        		</m:columns>
        		<m:items>
        			<m:ColumnListItem type="Active">
        				<m:cells>
        					<m:Text text="{DtOcorre}" />
        					<m:Text text="{TxNomelo}" />
        					<m:Text text="{DsOcorre}" />
        					<m:Text text="{DsStatus}" />
        				</m:cells>
        			</m:ColumnListItem>
        		</m:items>
